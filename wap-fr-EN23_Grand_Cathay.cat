        <profile name="3 Dud" typeId="419e-cc58-1a99-a0b0" typeName="Bomb Misfire Chart" hidden="false" id="31c5-98bf-0b2f-0f64" publicationId="65fb-a9da-0a71-9dc9" page="31">
