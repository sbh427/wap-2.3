  <sharedInfoGroups>
    <infoGroup name="Tactician" id="add3-af44-aa62-7956" hidden="false">
      <rules>
        <rule name="Tactitian" id="8777-f1f9-bee2-a2ab" hidden="false" publicationId="65fb-a9da-0a71-9dc9" page="13">
          <description>One friendly Human unit with the Yin and/or Yang special rule within 12&quot; may be given one of the Magistrate Tactics at the start of each of your turns by passing a Leadership test. Each bonus lasts until the start of your next turn. A unit can only benefit from one of these bonuses at a time.</description>
        </rule>
      </rules>
      <profiles>
        <profile name="Inspired Assault" typeId="b9cf-270e-8bc0-ee86" typeName="Magistrate Tactics" hidden="false" id="6b22-341d-331e-fb3a" publicationId="65fb-a9da-0a71-9dc9" page="13">
          <characteristics>
            <characteristic name="Effect" typeId="ab47-48ae-ce98-a7bc">The unit gains the Devastating Charge special rule.</characteristic>
          </characteristics>
        </profile>
        <profile name="Inspired Defence" typeId="b9cf-270e-8bc0-ee86" typeName="Magistrate Tactics" hidden="false" id="11f9-4ea8-31bb-4e14" publicationId="65fb-a9da-0a71-9dc9" page="13">
          <characteristics>
            <characteristic name="Effect" typeId="ab47-48ae-ce98-a7bc">Enemy units charging this unit to the front lose all their charge bonuses.</characteristic>
          </characteristics>
        </profile>
        <profile name="Inspired Marksmanship" typeId="b9cf-270e-8bc0-ee86" typeName="Magistrate Tactics" hidden="false" id="3e69-3258-6b9b-a7de" publicationId="65fb-a9da-0a71-9dc9" page="13">
          <characteristics>
            <characteristic name="Effect" typeId="ab47-48ae-ce98-a7bc">The unit ignores all negative non-magical Shooting Modifiers.</characteristic>
          </characteristics>
        </profile>
      </profiles>
    </infoGroup>
