                  <characteristics>
                    <characteristic name="Effect" typeId="b5e6-a050-5ffa-0bb5">A war machine engraved with a Rune of Penetration increases the Strength of its shots by 1, to a maximum of 10.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="points" value="30"/>
              </costs>
