              <characteristics>
                <characteristic name="Effect" typeId="b5e6-a050-5ffa-0bb5">The War Machine gains a Ward save (3+) against all missile attacks.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="20"/>
