    <profile id="b641-93cc-e21a-b9fc" name="Shem&apos;s Burning Gaze (Bound Spell 4+)" publicationId="9e23-79cb-pubN66727" page="150" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
