    <profile id="e0ba-3ab5-60ed-b508" name="Conjoined Homunculus" publicationId="1a01-044f-047b-618f" page="244" hidden="false" typeId="49dc-2fd8-b942-dbc2" typeName="Chaos Mutations and Powers">
