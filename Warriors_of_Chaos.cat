    <profile id="da22-8b80-a2a2-2479" name="Allure of Slaanesh" publicationId="1a01-044f-047b-618f" page="245" hidden="false" typeId="49dc-2fd8-b942-dbc2" typeName="Chaos Mutations and Powers">
