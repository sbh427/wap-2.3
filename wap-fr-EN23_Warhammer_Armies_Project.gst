      </characteristics>
    </profile>
    <profile id="f3f3-e999-3138-089b" name="The Vengeful Hood" publicationId="9e23-79cb-pubN66727" page="123" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
