              </characteristics>
            </profile>
            <profile name="06 (Mercenary Loyalty)" typeId="591c-24ac-19bd-3b4b" typeName="Mercenary Loyalty Table" hidden="false" id="bfaf-e5c1-c865-f3e9" publicationId="9e23-79cb-pubN116235" page="5">
              <characteristics>
