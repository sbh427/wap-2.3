            <profile name="Allowed Special Duplicates" typeId="3a6c-dc7b-2dc3-7a23" typeName="Special Duplicate Choice Table" hidden="false" id="6d6a-7bdc-129c-0242" publicationId="9e23-79cb-pubN66727" page="133">
              <characteristics>
