      </characteristics>
    </profile>
    <profile id="12de-92c8-df66-6049" name="The Wolf Hunts" publicationId="9e23-79cb-pubN66727" page="123" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
