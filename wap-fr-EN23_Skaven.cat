                <characteristic name="Special Rules" typeId="3df8-4ab0-c4bd-e2ad">Bound Spell, Power Level 5. One use only.</characteristic>
              </characteristics>
            </profile>
            <profile id="2dab-0451-691e-739a" name="Warpstorm" publicationId="e3da-3848-ff08-a482" page="46" hidden="false" typeId="cb92-d882-5ba8-11ab" typeName="Spell">
              <characteristics>
                <characteristic name="Spell Level" typeId="576b-eaa1-0bd1-3f7a">Bound Spell</characteristic>
                <characteristic name="Type" typeId="21e9-4aa2-f538-c93b">Direct Damage</characteristic>
                <characteristic name="Casting Value" typeId="d6bf-647e-ba69-e8a0">5+</characteristic>
                <characteristic name="Range" typeId="2954-76ea-decb-30ff">24&quot;</characteristic>
                <characteristic name="Details" typeId="f5f2-ac1b-25cd-89c6">Targets all units with Fly within range. The target units suffer D6 S 6 Hits with Lightning Attacks.</characteristic>
              </characteristics>
