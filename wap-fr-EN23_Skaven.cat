    <profile id="1694-20f0-8ca6-21cb" name="06 (Doom-flayer Mishap)" publicationId="4086-1ccd-f433-39a2" page="197" hidden="false" typeId="41a5-763e-72ee-4c16" typeName="Doom-flayer Mishap Chart">
