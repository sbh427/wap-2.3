            <profile name="Be the Mountain" typeId="4608-9b35-b7dc-202d" typeName="Prayer" hidden="false" id="253f-a051-278e-cfe5">
              <characteristics>
                <characteristic name="Effect" typeId="1f79-8144-edd6-8b51">The model and any unit they are with gain the Stubborn special rule until the start of your next turn.</characteristic>
              </characteristics>
            </profile>
            <profile name="Borne on the Wind" typeId="4608-9b35-b7dc-202d" typeName="Prayer" hidden="false" id="b792-0c0d-a9f8-f506">
